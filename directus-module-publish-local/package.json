{
  "name": "@dwaring87/directus-module-publish-local",
  "version": "1.0.0",
  "description": "A Directus v9 module for building a local site from the Directus app",
  "main": "src/index.js",
  "scripts": {
    "build": "npx directus-extension build",
    "deploy": "ext_path=\"$DIRECTUS_EXTENSIONS_DIR/modules/dwaring87-publish-local/\"; echo \"Deploying module to: $ext_path\"; mkdir -p \"$ext_path\"; cp ./dist/index.js \"$ext_path/index.js\""
  },
  "repository": {
    "type": "git",
    "url": "github.com:dwaring87/directus-extension-publish-local"
  },
  "author": "David Waring <dev@davidwaring.net>",
  "license": "MIT",
  "devDependencies": {
    "@directus/extensions-sdk": "^9.0.0-rc.92"
  },
  "directus:extension": {
    "type": "module",
    "path": "dist/index.js",
    "source": "src/index.js",
    "host": "^9.0.0-rc.92",
    "hidden": false
  }
}
